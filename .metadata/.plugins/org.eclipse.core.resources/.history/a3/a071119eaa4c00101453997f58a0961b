import org.springframework.web.bind.annotation.RequestMapping;

@Controller
public class MyController {
	
	@RequestMapping("/")
	public String idx() {
		
		return "index2";
	}
	
	@RequestMapping("view")
	public String view() {
		
		return "view";
	}
}
